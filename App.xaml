<Application x:Class="Keyboard_simulator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Keyboard_simulator"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources HAML\KeyboardKeyStyle.xaml"/>
                <ResourceDictionary Source="Resources HAML\Theme\Light.xaml"/>
                <ResourceDictionary Source="Resources HAML\MenuButtons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
